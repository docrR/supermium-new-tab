{
    "manifest_version": 2,
    "name": "Old New Tab",
    "short_name": "ONT",
    "description": "Replaces the new tab page in an attempt to replicate the old new tab page.",
    "default_locale": "en",
    "version": "0.1.0",
    "chrome_url_overrides": {
        "newtab": "main.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "topSites", // Get the top sites
        "management", // Get the list of apps
        "bookmarks", // Get the list of bookmarks
        "chrome://favicon/"
    ],
    "icons": {
        "200": "images/icon200.png",
        "128": "images/icon128.png",
        "19":"images/icon19.png"
    },
    "incognito": "split",
    "minimum_chrome_version" : "22"
}
