{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Supermium New Tab",
  "short_name": "SNT",
  "description": "Manifest V3 users, report to the brig.",
  "default_locale": "en",
  "version": "0.1.3",
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "permissions": ["storage", "topSites", "management", "bookmarks",
                  "chrome://favicon/"],
  "icons": {
    "200": "images/icon200.png",
    "128": "images/icon128.png",
    "19": "images/icon19.png"
  },
  "incognito": "split",
  "minimum_chrome_version": "122"
}
